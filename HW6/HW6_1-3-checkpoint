    "# Calculate the average price of books with a rating higher than 4.7 using RDD\n",
    "filtered_rdd = df.rdd.filter(lambda row: float(row[\"User Rating\"]) > 4.7)\n",
    "prices_rdd = filtered_rdd.flatMap(lambda row: [float(row[\"Price\"])] if row[\"Price\"].replace('.', '', 1).isdigit() else [])\n",
    "average_price = prices_rdd.mean()\n",
    "print(\"Average price of books with rating higher than 4.7:\", average_price)"
