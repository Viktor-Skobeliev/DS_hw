   "source": [
    "from pyspark.sql.functions import year, avg, count\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Витягуємо рік з колонки \"Year\"\n",
    "df = df.withColumn(\"Year\", year(df[\"Year\"]))\n",
    "\n",
    "# Обчислюємо середню кількість рецензій на рік\n",
    "reviews_per_year = df.groupBy(\"Year\").agg(avg(\"Reviews\").alias(\"AvgReviews\"))\n",
    "\n",
    "# Конвертуємо результат у Pandas DataFrame для візуалізації\n",
    "reviews_per_year_pd = reviews_per_year.toPandas()\n",
    "\n",
    "# Будуємо гістограму середньої кількості рецензій за роками\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.bar(reviews_per_year_pd[\"Year\"], reviews_per_year_pd[\"AvgReviews\"])\n",
    "plt.xlabel(\"Рік\")\n",
    "plt.ylabel(\"Середня кількість рецензій\")\n",
    "plt.title(\"Середня кількість рецензій за роками\")\n",
    "plt.show()\n",
    "\n",
    "# Обчислюємо кількість книг у жанрах \"Fiction\" та \"Non Fiction\"\n",
    "genre_counts = df.groupBy(\"Genre\").agg(count(\"Name\").alias(\"BookCount\"))\n",
    "\n",
    "# Конвертуємо результат у Pandas DataFrame для візуалізації\n",
    "genre_counts_pd = genre_counts.toPandas()\n",
    "\n",
    "# Будуємо окремі гістограми для кількості книг у жанрах \"Fiction\" та \"Non Fiction\"\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.bar(genre_counts_pd[\"Genre\"], genre_counts_pd[\"BookCount\"])\n",
    "plt.xlabel(\"Жанр\")\n",
    "plt.ylabel(\"Кількість книг\")\n",
