   "source": [
    "from pyspark.sql.functions import col, dense_rank\n",
    "from pyspark.sql.window import Window\n",
    "\n",
    "# Створити вікно для ранжування за рейтингом\n",
    "window = Window.orderBy(col(\"User Rating\").asc())\n",
    "\n",
    "# Знайти трьох авторів з найнижчим рейтингом\n",
    "authors_with_min_ratings = df.select(\"Author\", \"User Rating\") \\\n",
    "    .withColumn(\"rank\", dense_rank().over(window)) \\\n",
    "    .filter(col(\"rank\") <= 3) \\\n",
    "    .select(\"Author\") \\\n",
    "    .distinct()\n",
    "\n",
    "print(\"Троє авторів з найнижчим рейтингом:\")\n",
    "authors_with_min_ratings.show(truncate=False)\n"
   ]
